{
    "geode": "4.9.0",
    "gd": {
        "win": "1.920",
        "android": "1.920"
    },
    "id": "km7dev.audio-offset",
    "name": "Audio Offset Calculator",
    "version": "1.1.4",
    "developer": "km7dev",
    "description": "Easily set your audio offset",
    "resources": {
        "files": [
            "resources/sfx/*.ogg"
        ]
    },
    "links": {
        "source": "https://github.com/kingminer7/audiooffsetcalculator",
        "community": "https://discord.gg/Vj5Mcennph",
        "homepage": "https://km7dev.tech"
    },
    "issues": {
        "info": "Report issues in my Discord server.",
        "url": "https://discord.gg/Vj5Mcennph"
    },
    "settings": {
        "button": {
            "type": "custom:button",
            "name": "Sync Audio Offset"
        },
        "audio-cycles": {
            "type": "int",
            "default": 8,
            "min": 4,
            "max": 64,
            "control": {
                "input": true,
                "slider": true,
                "slider-step": 1
            },
            "name": "Audio beats",
            "description": "How many beats to use to determine your offset. 8 or above is recommended."
        }
    },
    "tags": [
        "utility",
        "music"
    ]
}
